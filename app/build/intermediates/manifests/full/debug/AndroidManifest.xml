<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rememberword"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.rememberword.MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.firstpeople.wordlearn.dictmanager.DictListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.firstpeople.wordlearn.dictmanager.DictManagerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.firstpeople.wordlearn.dictmanager.DirSelectActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.rememberword.testGesture" />
        <activity
            android:name="com.example.rememberword.WordsNote_Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.rememberword.AllWords_Activity"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.rememberword.KnowWell_Activity" />

        <service android:name="com.example.rememberword.ZdLockService" />

        <activity android:name="com.example.rememberword.LockScreen_Activity" />
    </application>

</manifest>