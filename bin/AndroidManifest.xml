<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rememberword"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="MainActivityBk"
            android:label="@string/title_activity_main" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.firstpeople.wordlearn.dictmanager.DictListActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.firstpeople.wordlearn.dictmanager.DictManagerActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.firstpeople.wordlearn.dictmanager.DirSelectActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="testGesture"></activity>
        <activity android:name=".WordsNote_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name=".AllWords_Activity" android:screenOrientation="portrait"></activity>
        <activity android:name="KnowWell_Activity"></activity>
        <service android:name="ZdLockService"></service>
        <activity android:name="LockScreen_Activity"></activity>
        
    </application>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.DISABLE_KEYGUARD"></uses-permission>   
	<uses-permission android:name="android.permission.VIBRATE"></uses-permission>
	
</manifest>